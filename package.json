{
  "name": "@exkit/routekit",
  "version": "1.0.1",
  "description": "Simple Express route handler for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts"
  },
  "keywords": [
    "express",
    "router",
    "middleware",
    "typescript"
  ],
  "author": "Wafula Colin Lubembe",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "typescript": "^5.8.3",
    "ts-node": "^10.9.2"
  }
}
