{
  "name": "@exkit/routekit",
  "version": "1.0.0",
  "description": "Simple Express route handler for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "express",
    "router",
    "middleware",
    "typescript"
  ],
  "author": "Wafula Colin Lubembe",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.2",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "semantic-release": "^24.2.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinmarklubembe/routekit.git"
  },
  "homepage": "https://github.com/colinmarklubembe/routekit#readme",
  "bugs": {
    "url": "https://github.com/colinmarklubembe/routekit/issues"
  }
}
